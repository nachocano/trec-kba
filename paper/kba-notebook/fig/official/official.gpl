# Chart settings
set key right bottom
# Input file contains comma-separated values fields
set datafile separator ","

set terminal pdf enhanced linewidth 2.0 size 3in,2in  
set output "official-prec.pdf"
# XYChart settings
set nologscale
set xr [500:1000] reverse
set yr [*:*] noreverse
set xlabel "Cutoff"
set ylabel "Precision"
# XYData Plotting
plot \
'uw-f_basic_single-ccr-all-entities-vital-require-positives=4-any-up-cutoff-step-size-50.txt' using 1:2 title "basic single" with lines, \
'uw-f_basic_multi-ccr-all-entities-vital-require-positives=4-any-up-cutoff-step-size-50.txt' using 1:2 title "basic multi" with lines, \
'uw-f_emb_comb-ccr-all-entities-vital-require-positives=4-any-up-cutoff-step-size-50.txt' using 1:2 title "embedding (comb)" with lines, \
'uw-f_emb_pos-ccr-all-entities-vital-require-positives=4-any-up-cutoff-step-size-50.txt' using 1:2 title "embedding (multi)" with lines, \
'uw-f_mean_dyn-ccr-all-entities-vital-require-positives=4-any-up-cutoff-step-size-50.txt' using 1:2 title "emb+staleness" with lines, \
